{
  "display_information": {
    "name": "Hubot-RS"
  },
  "features": {
    "bot_user": {
      "display_name": "Hubot-RS",
      "always_online": true
    }
  },
  "oauth_config": {
    "scopes": {
      "user": [
        "links:read",
        "channels:read",
        "groups:read",
        "channels:history",
        "groups:history",
        "im:history",
        "mpim:history",
        "reactions:read"
      ],
      "bot": [
        "app_mentions:read",
        "channels:history",
        "channels:join",
        "channels:manage",
        "channels:read",
        "channels:write.invites",
        "channels:write.topic",
        "chat:write",
        "chat:write.customize",
        "chat:write.public",
        "commands",
        "conversations.connect:read",
        "conversations.connect:write",
        "emoji:read",
        "files:read",
        "files:write",
        "groups:read",
        "groups:write",
        "groups:write.invites",
        "groups:write.topic",
        "im:history",
        "groups:history",
        "im:read",
        "im:write",
        "im:write.invites",
        "im:write.topic",
        "incoming-webhook",
        "metadata.message:read",
        "links:read",
        "links:write",
        "links.embed:write",
        "mpim:history",
        "mpim:read",
        "mpim:write",
        "mpim:write.invites",
        "mpim:write.topic",
        "pins:read",
        "pins:write",
        "reactions:read",
        "reactions:write",
        "users:read"
      ]
    }
  },
  "settings": {
    "event_subscriptions": {
      "user_events": [
        "link_shared",
        "member_left_channel",
        "message.app_home",
        "message.channels",
        "message.groups",
        "message.im",
        "message.mpim",
        "reaction_added",
        "reaction_removed"
      ],
      "bot_events": [
        "app_mention"
      ]
    },
    "interactivity": {
      "is_enabled": true
    },
    "org_deploy_enabled": true,
    "socket_mode_enabled": true,
    "token_rotation_enabled": false
  }
}